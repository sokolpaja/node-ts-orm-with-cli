{
  "name": "ts-orm-with-cli",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development debug=* NODE_PATH=./src nodemon --exec  ts-node src/server.ts",
    "migrate:create": "npx sequelize-cli migration:create --name",
    "seed:create": "npx sequelize-cli seed:generate --name",
    "seed": "npx sequelize-cli db:seed:all",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "build": "rimraf dist && tsc",
    "start": "npm run build && NODE_ENV=production NODE_PATH=./src node dist/src/server.js",
    "prepare": "husky install",
    "ts-patch": "npx ts-patch install -s",
    "fix-env": "export NODE_ENV=development"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.12",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.7"
  }
}
